left <- seq(-3.5,3.5,by=.1)
up <- seq(0.00,.09,by=.01)

join <- function(x,y) {
    #x left
    #y up
    #t value for pnorm
    if (x < 0) {
        t <- x - y
    } else {
        t <- x + y
    }
    pnorm(t)
}

